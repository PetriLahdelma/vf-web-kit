{
  "name": "vf-web-kit",
  "version": "0.1.1",
  "description": "Build production-ready variable font web kits with subsetting, CSS, tokens, and a specimen.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PetriLahdelma/vf-web-kit.git"
  },
  "homepage": "https://github.com/PetriLahdelma/vf-web-kit#readme",
  "bugs": {
    "url": "https://github.com/PetriLahdelma/vf-web-kit/issues"
  },
  "keywords": [
    "variable-font",
    "font",
    "subset",
    "webfonts",
    "css",
    "specimen"
  ],
  "type": "module",
  "bin": {
    "vf-web-kit": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "lint": "tsc -p tsconfig.json --noEmit",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "brand:build": "node scripts/brand-build.mjs",
    "brand:screenshots": "node scripts/brand-screenshots.mjs",
    "brand:demo": "node scripts/brand-demo.mjs"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "commander": "11.1.0",
    "fast-glob": "3.3.2",
    "fontkit": "2.0.2",
    "subset-font": "2.4.0"
  },
  "devDependencies": {
    "@types/node": "20.11.30",
    "typescript": "5.4.5",
    "vitest": "1.4.0",
    "svgo": "^3.3.2",
    "sharp": "^0.33.5"
  }
}
